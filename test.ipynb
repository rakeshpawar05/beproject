    "tree = ET.parse('sample.xml')\n",
    "root = tree.getroot()\n",
    "\n",
    "def tprint(obj,indent=\"->\"):\n",
    "    print(indent,obj.tag,obj.text.strip())\n",
    "    for o in obj:\n",
    "        tprint(o,\"    \"+indent)\n",
    "\n",
